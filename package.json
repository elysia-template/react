{
    "name": "elysia-react",
    "type": "module",
    "version": "1.1.0",
    "module": "dist/server.js",
    "dependencies": {
        "elysia": "^1.1.24",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.27.0"
    },
    "scripts": {
        "build": "bunx vite build && bun build --minify-whitespace --minify-syntax --target bun --outfile ./dist/server.js ./server.ts",
        "start": "[ -f ./dist/server.js ] || ( echo '' && echo 'Warning: No build detected in project.' && echo '' && echo '    -> bun run build' && echo '    -> bun run start' && echo '' ); [ -f ./dist/server.js ] && NODE_ENV=production bun ./dist/server.js",
        "dev": "bunx vite & bun run --watch server.ts",
        "lint": "bunx eslint ."
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/bun": "^1.1.12",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.13.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "globals": "^15.11.0",
        "postcss": "^8.4.47",
        "tailwindcss": "^3.4.14",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.11.0",
        "vite": "^5.4.10"
    }
}